{
    "name": "eminiarts/nova-permissions",
    "description": "Laravel Nova Grouped Permissions.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "spatie/laravel-permission": "^6"
    },
    "autoload": {
        "psr-4": {
            "Eminiarts\\NovaPermissions\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eminiarts\\NovaPermissions\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
